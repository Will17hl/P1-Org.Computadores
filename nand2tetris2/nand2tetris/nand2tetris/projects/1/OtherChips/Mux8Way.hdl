CHIP Mux8Way {

    IN a, b, c, d, e, f, g, h, sel[3];
    OUT out;

    PARTS:
    Mux(a=a, b=b, sel=sel[2], out=Mux1);
    Mux(a=c, b=d, sel=sel[2], out=Mux2);
    Mux(a=e, b=f, sel=sel[2], out=Mux3);
    Mux(a=g, b=h, sel=sel[2], out=Mux4);

    Mux(a=Mux1, b=Mux2, sel=sel[1], out=Mux12);
    Mux(a=Mux3, b=Mux4, sel=sel[1], out=Mux34);

    Mux(a=Mux12, b=Mux34, sel=sel[0], out=out);
    
}